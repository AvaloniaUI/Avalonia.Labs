<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:Avalonia.Labs.Catalog"
             xmlns:labs="using:Avalonia.Labs.Controls"
             x:Class="Avalonia.Labs.Catalog.App"
             RequestedThemeVariant="{x:Static ThemeVariant.Light}">
    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>

    <Application.Styles>
        <FluentTheme/>
        <labs:ControlThemes/>
    </Application.Styles>
</Application>
